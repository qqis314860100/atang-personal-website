    "restart:socket": "pm2 restart socket-server",
    "logs:socket": "pm2 logs socket-server",
    "status:socket": "pm2 status",
    "analyze": "ANALYZE=true next build",
    "optimize": "node scripts/optimize-performance.js",
    "optimize:win": "node scripts/optimize-performance-win.js",
    "clean": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo logs/*.log",
